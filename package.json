{
  "name": "remix-devtools",
  "version": "0.0.0",
  "description": "Devtools for remix-run framework.",
  "keywords": [
    "remix-run",
    "remix",
    "react",
    "devtools",
    "dev"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "types": "./dist/index.d.ts",
      "node": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      },
      "types": "./dist/server.d.ts",
      "node": "./dist/server.cjs",
      "default": "./dist/server.js"
    },
    "./index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "author": "Siddhant Gupta <me@guptasiddhant.com>",
  "license": "MIT",
  "dependencies": {
    "remix-development-tools": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuptaSiddhant/remix-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/Code-Forge-Net/Remix-Dev-Tools/issues"
  },
  "homepage": "https://github.com/Code-Forge-Net/Remix-Dev-Tools",
  "scripts": {
    "prepublishOnly": "node setup.js"
  }
}
